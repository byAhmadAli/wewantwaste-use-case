{
    "source": {
        "include": ["src"], 
        "includePattern": ".+\\.jsx?$" 
    },
    "plugins": ["plugins/markdown"], 
    "opts": {
        "recurse": true,
        "destination": "public/docs"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}